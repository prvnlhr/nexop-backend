{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "@generated/*": ["src/generated/*"]
    }
  },
  "include": ["src/**/*", "src/generated/prisma/**/*"],
  "exclude": ["node_modules", "dist"]
}